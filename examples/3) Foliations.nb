Cell[BoxData[
 RowBox[{
  RowBox[{"cqmgPlot", "[", 
   RowBox[{"Xsfz4", ",", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{

 RowBox[{
  RowBox[{"Tiling", "=", 
   RowBox[{"Tuples", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "10"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"-", "10"}], ",", "0"}], "}"}]}], "}"}], ",", "3"}], "]"}]}],
   ";"}]], "Input",

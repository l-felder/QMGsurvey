   3.8869342025047665`*^9, {3.886935585393769*^9, 3.8869355859480104`*^9}, 
   3.8869357698225207`*^9, 3.886935852378523*^9, {3.8869960350947375`*^9, 
   3.8869960448480616`*^9}, {3.8869996096412263`*^9, 3.8869996129095345`*^9}, 
   3.887000122121189*^9},

   3.8869960334655724`*^9}, {3.8869996072234097`*^9, 3.8869996073234797`*^9}},
 CellLabel->
  "In[133]:=",ExpressionUUID->"eddbe0e4-7d4b-4e9c-8bd8-763edee5cfb3"],

Cell[BoxData[
 RowBox[{
  RowBox[{"outB5", "=", 
   RowBox[{"cqmgIntegrateTilingPreview", "[", 
    RowBox[{
    "Xsfz4", ",", "outA5", ",", "Tiling", ",", "0.015", ",", "50", ",", "0", 
     ",", "2", ",", "\"\<TSleaf\>\"", ",", "\"\<omega\>\""}], "]"}]}], 
  ";"}]], "Input",
